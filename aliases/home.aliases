alias cd-src="cd ~/src"
alias cd-dot="cd ~/src/dot-files"
alias cd-p="cd ~/p"

alias ll="ls -lahL"

alias megapush="git push origin master;git push mainframe master"

alias resetdots="git -C ~/src/dot-files reset --hard HEAD"
alias savedots="git -C ~/src/dot-files add -A;git -C ~/src/dot-files commit -m 'Saving dots.';git -C ~/src/dot-files push mainframe master"

alias sz="source ~/.zshrc"
alias vz="vim ~/.zshrc"
